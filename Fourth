function calculatePerimeter(a, b, c) {
  // Calculate perimeter
  let perimeter = a + b + c;

  // Return the result
  return perimeter;
}

function output(params) {
  let result = calculatePerimeter(5, 2, 2);
  console.log("The perimeter of the triangle is:", result);
}

output();

**************OUTPUT*********************
number4:11 The perimeter of the triangle is: 9



